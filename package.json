{
  "name": "web-render-api",
  "version": "1.0.0",
  "description": "Production-grade Web Rendering API with browser pooling and proxy support",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "install:browsers": "npx playwright install chromium"
  },
  "keywords": [
    "web-scraping",
    "browser-rendering",
    "playwright",
    "proxy",
    "headless-browser"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/compress": "^7.0.3",
    "@fastify/cors": "^9.0.1",
    "@fastify/static": "^7.0.4",
    "fastify": "^4.28.1",
    "pino": "^8.19.0",
    "pino-pretty": "^11.0.0",
    "playwright": "^1.42.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.28",
    "@types/uuid": "^9.0.8",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "vitest": "^1.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
